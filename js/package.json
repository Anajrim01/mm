{
  "name": "@dqbd/tiktoken",
  "version": "0.4.0",
  "description": "Javascript bindings for tiktoken",
  "license": "Apache-2.0",
  "main": "node/_tiktoken.js",
  "types": "node/_tiktoken.d.ts",
  "exports": {
    ".": {
      "node": {
        "types": "./node/_tiktoken.d.ts",
        "default": "./node/_tiktoken.js"
      },
      "default": {
        "types": "./bundler/_tiktoken.d.js",
        "default": "./bundler/_tiktoken.js"
      }
    },
    "./init": "./init.js",
    "./tiktoken.wasm": {
      "types": "./tiktoken.wasm.d.ts",
      "default": "./tiktoken.wasm"
    }
  },
  "scripts": {
    "build": "rm -rf dist/ ranks/ && yarn run ranks && yarn run build:node && yarn run build:bundler && yarn run build:cleanup",
    "ranks": "tsx scripts/download_ranks.ts",
    "build:bundler": "wasm-pack build --target bundler --release --out-dir dist/bundler && rm dist/bundler/.gitignore",
    "build:node": "wasm-pack build --target nodejs --release --out-dir dist/node && rm dist/node/.gitignore",
    "build:cleanup": "tsx scripts/override_any.ts",
    "test": "yarn vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dqbd/tiktoken"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.14.4",
    "ts-morph": "^17.0.1",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
}
